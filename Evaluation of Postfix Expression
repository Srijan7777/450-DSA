class Solution
{
    public:
    //Function to evaluate a postfix expression.
    int evaluatePostfix(string S)
    {
        // Your code here
        stack<int>s;
        for(int i=0;i<S.size();i++)
        {
        if(S[i]>='0' && S[i]<='9')
        {
            s.push(S[i]-'0');
        }
        else
        {
        int a= s.top();
        s.pop();
        int b= s.top();
        s.pop();
            if(S[i]=='*')
            {
                s.push(a*b);
            }
            else if(S[i]=='/')
            {
                s.push(b/a);
            }
            
            else if(S[i]=='+')
            {
                s.push(b+a);
            }
            else if(S[i]=='-')
            {
                s.push(b-a);
            }
        
        }        
        }
    
        return s.top();
    }
};
